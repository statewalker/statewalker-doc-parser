{
  "name": "@statewalker/parse-mdoc",
  "version": "0.0.1",
  "description": "A simple HTML Parser",
  "keywords": [],
  "homepage": "https://github.com/statewalker/statewalker-parse-mdoc",
  "author": {
    "name": "Mikhail Kotelnikov",
    "email": "mikhail.kotelnikov@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/package.json",
    "dist/**/*.js",
    "src/**/*.js",
    "index.js"
  ],
  "module": "src/index.js",
  "main": "src/index.js",
  "jsdelivr": "dist/index.js",
  "unpkg": "dist/index.js",
  "exports": {
    "umd": "./dist/index-umd.min.js",
    "default": "./src/index.js"
  },
  "dependencies": {
    "@statewalker/fsm": "^0.16.0",
    "@statewalker/tree": "^0.10.2",
    "uc.micro": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "eslint": "^8",
    "expect.js": "^0.3",
    "mocha": "^10",
    "rollup": "^3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:statewalker/statewalker-parse-mdoc.git"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"**/*.ts\"",
    "test-vitest": "vitest --watch",
    "test": "mocha -R spec ./test/index.js && yarn eslint",
    "prepublishOnly": "rm -rf dist && yarn test && yarn build"
  },
  "sideEffects": false,
  "publishConfig": {
